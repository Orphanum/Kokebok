var responsiveImages=function(){var n=-1,t,r=[{min:-1,max:480},{min:481,max:767},{min:768,max:959},{min:960,max:1e6}],u=function(t){var o,u,r,f,i,e;if(!t||!t.children)return!1;if(o=t.children[0],o){u=t.getAttribute("data-transformations");u=u.replace(/'/g,'"');var l=JSON.parse(u),a=t.getAttribute("data-fallback"),v=t.getAttribute("data-external-source"),y=t.getAttribute("data-lazy"),s=t.getAttribute("data-itemprop"),h=t.getAttribute("data-object-fit"),c=t.getAttribute("data-original-src");if(c){if(r=c,a||v||(f=l[n],f&&(r=r+"/"+f)),i=$(t).children("img"),i.length==0)i=document.createElement("img"),e=t.getAttribute("data-alt"),i.setAttribute("alt",e?e:""),s&&i.setAttribute("itemprop",s),h&&i.setAttribute("data-object-fit",h),i.src=r,t.removeChild(t.children[0]),t.appendChild(i);else{if(i=i[0],i.src==r)return r;i.src=r;y&&i.setAttribute("data-lazy",r)}return r}}return!1},f=function(){for(var n=0,i=r.length;n<i;n++)if(r[n].min<=t&&r[n].max>=t)return n;return-1},e=function(r){if(!r||(t=window.innerWidth,n=f(),n==-1))return!1;for(i=0;i<r.length;i++)u(r[i]);return!0};return{lazyloadImages:e,lazyloadImage:u,caculateCurrentBreakPoint:f}}();matprat.responsiveImages=document.querySelectorAll(".lazy-load");responsiveImages.lazyloadImages(matprat.responsiveImages)